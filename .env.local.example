# SuperTokens Core connection
CORE_URL=http://localhost:3567
# If your Core requires an API key, set it here; otherwise leave blank
CORE_API_KEY=
# Optional: override CDI version header if your Core requires it
# CORE_CDI_VERSION=4.0
# Optional: trust self-signed certs in dev (affects all fetch) - dev only
# CORE_INSECURE_TLS=false

# SuperTokens Dashboard (standalone helper server in this repo)
# Port the dashboard server listens on
DASHBOARD_PORT=3434
# API key to access the dashboard (set a strong value)
DASHBOARD_API_KEY=change-me-dashboard

# Passwordless SMS configuration (used by the helper dashboard server)
# Endpoint for your SMS service (HTTP POST)
SMS_API_URL=https://sms.bytecraft.ir/sms/send
# Token / API key for the SMS service (sent as Bearer token)
SMS_API_TOKEN=
# Optional sender name/number used by the service
SMS_SENDER=SuperTokens

# Admin UI credentials (for this web UI itself)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-me
